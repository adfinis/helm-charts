              serviceAccountName: RELEASE-NAME-openshift-etcd-backup
              tolerations:
                - effect: NoSchedule
                  key: node-role.kubernetes.io/master
              volumes:
                - hostPath:
                    path: /
                    type: Directory
                  name: host
                - hostPath:
                    path: /mnt/etcd-backups
                    type: Directory
                  name: volume-backup
      schedule: 0 0 * * *
      startingDeadlineSeconds: 600
